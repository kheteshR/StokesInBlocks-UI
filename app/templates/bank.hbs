 <main role="main" class="container">
      <div class="d-flex align-items-center p-3 my-3 text-white-50 bg-purple rounded box-shadow">
        <img class="mr-3" src="/assets/bank.png" alt="" width="48" height="48">
        <div class="lh-100">
          <h6 class="mb-0 text-white lh-100" >HDFC BANK</h6>
          <small>Since 1984</small>
        </div>
      </div>

      <div class="my-3 p-3 bg-white rounded box-shadow">
        <h6 class="border-bottom border-gray pb-2 mb-0"><strong>Recent Request Of Customers Updates</strong></h6>
        
        {{#each AllRequest as |data|}}
        <div class="media text-muted pt-3">
          <img data-src="holder.js/32x32?theme=thumb&bg=007bff&fg=007bff&size=1" alt="" class="mr-2 rounded">
          <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">
            <strong class="d-block text-gray-dark">Transaction Id:</strong>
            <b style="color:green">{{data.transactionHash}}</b>
            <br/>
            <br>
            <strong>Requester For Tokens:</strong><br/>
            <b style="color:green">{{data.Requester}}</b><br/>
            <br>
             <strong>Account of Requester:</strong><br>
            <b style="color:green">{{data.RequesterAddress}}</b><br/>
            <br/>
            <strong>Digital Identity Of customer:</strong><br/>
            <b style="color:green">{{data.RequesterDigitalIdentity}}</b><br/>
            <br/>
            <strong>Spending Amount Of Customer:</strong><br/>
            <b style="color:green">{{data.SpendingAmount}}INR</b><br/>
             <br/>
            <strong>Status:</strong><br/>
            <b style="color:green">{{data.status}}</b><br/>
           
                     </p>
        </div>
        <br>
        <div class="row">
          <div class="col-lg-6">
          <button type="button" class="btn btn-success" style="width:40%" {{action "ActionApprove" data._id "Accepted" "Condition Satisfied"}}>Yes</button>
            </div>
             <div class="col-lg-6">
               <button type="button" class="btn btn-danger" style="width:40%" {{action "ActionReject" data._id "Rejected" "Condition Not Satisfied"}}>No</button>
            </div>
          </div>
          <br>
          {{#if Rejected}} 
<div class="alert alert-danger">
   <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
  <strong>Oops!</strong>Rejected<br/>
  <strong>Your transaction Id:</strong><b>{{Actionhash}}</b>
</div>
{{/if}}

 {{#if Accepted}}
<div class="alert alert-success">
   <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
  <strong>Success!</strong>Accepted<br/>
  <strong>Your transaction Id:</strong><b>{{Actionhash}}</b>
</div>
{{/if}}
        {{/each}}
       
        <small class="d-block text-right mt-3">
          <a href="#">All updates</a>
        </small>
      </div>

      <div class="my-3 p-3 bg-white rounded box-shadow">
        <h6 class="border-bottom border-gray pb-2 mb-0">Suggestions</h6>
        <div class="media text-muted pt-3">
          <img data-src="holder.js/32x32?theme=thumb&bg=007bff&fg=007bff&size=1" alt="" class="mr-2 rounded">
          <div class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">
            <div class="d-flex justify-content-between align-items-center w-100">
              <strong class="text-gray-dark">Full Name</strong>
              <a href="#">Follow</a>
            </div>
            <span class="d-block">@username</span>
          </div>
        </div>
        <div class="media text-muted pt-3">
          <img data-src="holder.js/32x32?theme=thumb&bg=007bff&fg=007bff&size=1" alt="" class="mr-2 rounded">
          <div class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">
            <div class="d-flex justify-content-between align-items-center w-100">
              <strong class="text-gray-dark">Full Name</strong>
              <a href="#">Follow</a>
            </div>
            <span class="d-block">@username</span>
          </div>
        </div>
        <div class="media text-muted pt-3">
          <img data-src="holder.js/32x32?theme=thumb&bg=007bff&fg=007bff&size=1" alt="" class="mr-2 rounded">
          <div class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">
            <div class="d-flex justify-content-between align-items-center w-100">
              <strong class="text-gray-dark">Full Name</strong>
              <a href="#">Follow</a>
            </div>
            <span class="d-block">@username</span>
          </div>
        </div>
        <small class="d-block text-right mt-3">
          <a href="#">All suggestions</a>
        </small>
      </div>
    </main>
